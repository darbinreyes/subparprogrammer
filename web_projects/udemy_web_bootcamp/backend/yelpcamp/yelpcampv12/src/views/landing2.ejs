        <% if(successMessage && successMessage.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>

    <div id="landing-header">
    <h1>Welcome to YelpCamp!</h1>
